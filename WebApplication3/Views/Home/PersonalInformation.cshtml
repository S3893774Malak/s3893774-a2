@{
    ViewBag.HideTopNavBar = false;
}
@model WebApplication3.Models.Customer
<h3>Personal Information</h3>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>TFN</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Postcode</th>
            <th>Mobile</th>
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
       
            <tr>
            <td>@Model.Name</td>
            <td>@Model.TFN</td>
            <td>@Model.Address</td>
            <td>@Model.City</td>
            <td>@Model.State</td>
            <td>@Model.Postcode</td>
            <td>@Model.Mobile</td>
                <!-- Add other columns as needed -->
            </tr>
        
    </tbody>
</table>
