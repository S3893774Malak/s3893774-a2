@{
    ViewBag.HideTopNavBar = false;
}
@model List<WebApplication3.Models.Account>
<h1>Below are your account numbers</h1>
<table>
    <thead>
        <tr>
            <th>Account ID</th>
            <th>Account number</th>
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var acc in Model)
        {
            <tr>
                <td>@acc.Id</td>
                <td>@acc.AccountNumber</td>
                <!-- Add other columns as needed -->
            </tr>
        }
    </tbody>
</table>
